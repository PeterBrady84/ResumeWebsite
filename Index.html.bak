<!-- Smooth Scrolling -->
<script>
    $(function () {
        $('a[href*=#]:not([href=#]):not([href=#carousel-3-ball-strike]):not([href=#carousel-pizza]):not([href=#carousel-epic-search]):not([href=#carousel-fuel-logger]):not([href=#carousel-resume])').click(function () {
            if (location.pathname.replace(/^\//, '') == this.pathname.replace(/^\//, '')
                || location.hostname == this.hostname) {

                var target = $(this.hash);
                target = target.length ? target : $('[name=' + this.hash.slice(1) + ']');
                if (target.length) {
                    $('html,body').animate({
                        scrollTop: target.offset().top
                    }, 1000);
                    return false;
                }
            }
        });
    });
</script>

